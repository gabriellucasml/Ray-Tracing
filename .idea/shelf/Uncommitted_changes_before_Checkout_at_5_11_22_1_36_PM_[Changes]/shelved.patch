+                                color bl(0,0,0),tl(0,0,0),br(0,0,0),tr(0,0,0);
+                                std::stringstream sAuxBl, sAuxBr,sAuxTl,sAuxTr;
+                                for(int i = 0; i < 2; i++){
+                                    sAuxBl << auxbl;
