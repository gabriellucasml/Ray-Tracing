+                                    sAuxTl << auxtl;
+                                    sAuxTr << auxtr;
+                                    try {
+                                        bl[i] = std::stoi(sAuxBl.str());
+                                        br[i] = std::stoi(sAuxBr.str());
+                                        tl[i] = std::stoi(sAuxTl.str());
+                                        tr[i] = std::stoi(sAuxTr.str());
