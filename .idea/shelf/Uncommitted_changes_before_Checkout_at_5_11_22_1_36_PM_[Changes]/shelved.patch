+#include <sstream>
+#include "tinyxml2.h"
+
+struct RunningOptions{
+    bool  help = false;
+    std::string cameraType;
+    std::string filmType;
+    int filmX_res = 0;
+    int filmY_res = 0;
+    std::string filmFilename;
+    std::string filmImgtype;
+    std::string backgroundType;
+    std::string backgroundMapping;
